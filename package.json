  "bugs": {
    "url": "https://github.com/spsprinkles/news-ticker/issues"
  },
  "homepage": "https://github.com/spsprinkles/news-ticker#readme",
  "dependencies": {
    "datatables.net": "^1.13.4",
    "datatables.net-bs5": "^1.13.4",
    "dattatable": "^2.7.6",
    "gd-sprest-bs": "^10.10.26",
    "jquery": "^3.7.0",
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "babel-loader": "^9.1.2",
    "css-loader": "^6.8.1",
