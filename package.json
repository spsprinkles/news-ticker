{
  "name": "news-ticker",
  "version": "1.0.0",
  "description": "Displays news items in the header of a modern page.",
  "main": "src/index.ts",
  "scripts": {
    "all": "npm run clean && npm run build && npm run prod && npm run spfx",
    "build": "webpack --mode development",
    "clean": "node ./clean.js",
    "prod": "webpack --mode production",
    "serve": "npm run build && cd spfx && gulp serve",
    "spfx": "cd spfx && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spsprinkles/news-ticker.git"
  },
