{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "news-ticker-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/newsTicker/NewsTickerApplicationCustomizer.js",
          "manifest": "./src/extensions/newsTicker/NewsTickerApplicationCustomizer.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "main-lib": {
      "globalName": "main-lib",
      "path": "../dist/news-ticker.js"
    }
  },
  "localizedResources": {
    "NewsTickerApplicationCustomizerStrings": "lib/extensions/newsTicker/loc/{locale}.js"
  }
}